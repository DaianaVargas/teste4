<%- include('../header') %>


<!-- <table class="table table-hover table-dark"> -->
<div class="container">
  <!-- Content here -->
  <div class="alert alert-primary" role="alert">
    Publicações
  </div>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th scope="col">Publicação</th>
        <th scope="col">Ações</th>
      </tr>
    </thead>
    <tbody>
      <% publicacoes.forEach(publicacao => { %>
      <tr>
        <td><%= publicacao.titulo %></td>
        <td>
            <a href="/publicacoes/editar/<%= categoria %>/<%= publicacao.id %>">Editar</a>
            <a href="/publicacoes/excluir/<%= categoria %>/<%= publicacao.id %>">Excluir</a><br/>
        </td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>


<%- include('../footer') %>
